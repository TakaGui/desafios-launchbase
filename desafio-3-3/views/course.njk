{% extends 'layout.njk' %}

{% block head %}
  <title>Rocketseat - {{course.title}}</title>
{% endblock %}
  

{% block content %}
  <div id="wrapper" class="course">
    <h1>{{course.title}}</h1>
  </div>
  
  <section class="course">

    <div class="card" id="{{course.id}}">
      <div class="card__page-container">
        <iframe src="https://rocketseat.com.br/{{course.id}}" frameborder="0"></iframe>
      </div> 
    </div>

  </section> <!-- End section cards -->
{% endblock %}

{% block server %}
  <script src="./scripts.js"></script>
{% endblock %}